select sum(1)
from ba_ups.mt_user_lbs_school_features a
full outer join ba_ups.mt_user_lbs_hanjia_features b
on a.userid=b.userid

full outer join ba_ups.mt_user_lbs_shujia_features c
on a.userid=c.userid

full outer join ba_ups.mt_user_lbs_holiday_features d
on a.userid=d.userid

where a.dt='2016-08-26' and b.dt='2016-08-26' and c.dt='2016-08-26' and d.dt='2016-08-26'
